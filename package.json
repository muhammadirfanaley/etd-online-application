{
  "name": "etd",
  "version": "1.0.0",
  "description": "etdf online application.",
  "repository": "http://islamabadexcise.gov.pk/",
  "author": "zeeshan",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "run-s dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development webpack-dev-server --config webpack.debug.js",
    "build": "run-s lint build:no-lint",
    "analyze": "run-s lint clean build:analyze",
    "build:no-lint": "run-s clean build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config webpack.analyze.js",
    "clean": "rimraf dist",
    "lint": "run-p lint:js lint:style",
    "lint:js": "eslint \"**/*.js\"",
    "lint:style": "stylelint \"**/*.@(css|sass|scss)\"",
    "format": "run-p format:js format:style format:others",
    "format:js": "prettier-eslint --write \"**/*.js\"",
    "format:style": "run-s format:style:prettier format:style:stylelint",
    "format:others": "prettier --write \"**/*.@(json|gql|graphql)\"",
    "format:style:prettier": "prettier --write \"**/*.@(css|sass|scss)\"",
    "format:style:stylelint": "stylelint \"**/*.@(css|sass|scss)\" --fix"
  },
  "lint-staged": {
    "**/*.@(js|jsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(css|sass|scss)": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "**/*.@(json|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "axios": "^0.21.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "immer": "^1.9.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.11",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-countdown-now": "^2.0.2",
    "react-dom": "^16.7.0",
    "react-md": "^1.9.2",
    "react-redux": "^6.0.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "tiny-cookie": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.2.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.2",
    "eslint-plugin-standard": "^4.0.0",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "fast-async": "^7.0.6",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1",
    "react-dev-utils": "^7.0.0",
    "react-hot-loader": "^4.6.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
